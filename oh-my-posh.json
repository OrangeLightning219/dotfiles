{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "purple": "#8981d6",
    "red": "#eb6f92",
    "salmon": "#ea9a97",
    "white": "#FFFFFF"
  },
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": false
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "p:white",
          "background": "p:red",
          "type": "python",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_full"
          },
          "template": " {{ .Path }} ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:purple",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "template": " âžœ ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "foreground": "p:white",
          "powerline_symbol": "\ue0b0",
          "background": "p:salmon",
          "type": "git",
          "style": "powerline"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
